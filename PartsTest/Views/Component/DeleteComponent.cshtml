
@{
    ViewBag.Title = "DeleteComponent";
}
<h3>Delete Component</h3>

    <form>

        <div class="form-group col-md-8">
            <div class="col-md-2">
                <input type="hidden" ng-model="Component.Id" class="form-control" disabled />

                <label for="Name">Name: </label>
            </div>
            <div class="col-md-6">
                <input type="text" ng-model="Component.Name" class="form-control" disabled/>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="col-md-2">
                <label for="Type">Type: </label>
            </div>
            <div class="col-md-6">
                <input type="text" ng-model="Component.Type" class="form-control" disabled/>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="col-md-2">
                <label for="Price">Price: </label>
            </div>
            <div class="col-md-6">
                <input type="text" ng-model="Component.Price" class="form-control" disabled/>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="col-md-2">
                <label for="Country">Country: </label>
            </div>
            <div class="col-md-6">
                <input type="text" ng-model="Component.Country" class="form-control" disabled/>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="col-md-2">
                <label for="DueDate">DueDate: </label>
            </div>
            <div class="col-md-6">
                <input type="text" ng-model="Component.DueDate" class="form-control" disabled/>
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="col-md-2">
                <input type="button" class="btn btn-primary" value="Delete" ng-click="delete()" />
            </div>
        </div>
        <div class="form-group col-md-8">
            <div class="error">{{error}}</div>

        </div>
    </form>




